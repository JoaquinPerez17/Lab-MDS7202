    "rnd_forest = RandomForestClassifier(class_weight=\"balanced\") # Random Forest\n",
    "pipe_rnd_forest = Pipeline([\n",
    "    ('scaler', ct),\n",
    "    ('classifier', rnd_forest)\n",
    "    ])"
